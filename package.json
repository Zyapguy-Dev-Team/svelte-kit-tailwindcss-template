{
  "name": "svelte-kit-tailwind-template",
  "version": "1.0.0",
  "description": "A template with SvelteKit, TailwindCSS, Vite, TypeScript, Adapter-Node etc. pre-configured. This template uses PNPM and it's recommended to do the same.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev --host localhost --port 3000",
    "build": "cross-env NODE_ENV=production svelte-kit build",
    "preview": "svelte-kit preview --host localhost --port 3000",
    "start": "cross-env HOST=localhost PORT=3000 node dist/index.js",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.42",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "autoprefixer": "^10.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "postcss": "^8.3.6",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "tailwindcss": "^2.2.7",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zyapguy-Dev-Team/svelte-kit-tailwindcss-template.git"
  },
  "keywords": [
    "svelte",
    "svelte-kit",
    "tailwind",
    "tailwindcss",
    "typescript",
    "adapter-node",
    "template"
  ],
  "author": "debarchitodev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Zyapguy-Dev-Team/svelte-kit-tailwindcss-template/issues"
  },
  "homepage": "https://github.com/Zyapguy-Dev-Team/svelte-kit-tailwindcss-template#readme"
}
